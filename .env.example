APP_NAME=Laravel
# アプリケーション名
APP_ENV=local
# 環境設定、ローカル以外に本番環境：productionがある。
APP_KEY=
# 暗号化に使われるキー
APP_DEBUG=true
# エラー画面を表示するかしないか
APP_URL=http://localhost
# URL設定

LOG_CHANNEL=stack
# ログ情報の設定
# stack　・・・　複数のログを設定可能にする（デフォルト値）

DB_CONNECTION=mysql
# データベースの種類
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=root
DB_PASSWORD=
# 利用するデータベース名にアクセスするパスワードです。

BROADCAST_DRIVER=log
CACHE_DRIVER=file
# 利用するキャッシュの方式を設定する項目です。
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_DRIVER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS=null
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1

MIX_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
MIX_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

# envファイルの内容についてはこちら
# https://blog.capilano-fw.com/?p=1382#APP_DEBUG
